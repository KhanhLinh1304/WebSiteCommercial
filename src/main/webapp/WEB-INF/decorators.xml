<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
	<excludes>
		<pattern>/api*</pattern>
	</excludes>
	<decorator name="admin" page="admin.jsp">
		<pattern>/admin*</pattern>
	</decorator>
	<decorator name="web" page="web.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="login" page="login.jsp">
		<pattern>/login</pattern>
	</decorator>
	<decorator name="register" page="signup.jsp">
		<pattern>/register</pattern>
	</decorator>
	<decorator name="send-mail-register" page="sendCodeRegister.jsp">
		<pattern>/send-mail-register</pattern>
	</decorator>
</decorators>
